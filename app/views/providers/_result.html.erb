<div class="row">
  <div class="col-sm-3">
  </div>
  <div class="col-sm-6" id="result-partial-container">
    <div class="row">
      <div class="col-sm-3">
        <img src=<%= provider.photo_url %> class="img-rounded"/>
      </div>
      <div class="col-sm-6">
        <h3><%= provider.name %></h3>
        <p><%= provider.email %></p>
        <% provider.assessments.each do |a| %>
          <span><%= a.word %></span>
        <% end %>
      </div>
      <div class="col-sm-3">
        <p><%= provider.phone_number %></p>
      </div>
    </div>
  </div>
</div>


