
<div class="container-fluid" id="input-container">
   <div class="row">
    <div class="col-sm-3">
      <h1>Spring</h1>
    </div>
   </div>

  <div class="row">
    <div class="col-sm-4">
    </div>
    <div class="col-sm-4">
      <h3>Today I feel...</h3>
    </div>
  </div>

  <% 4.times do %>
    <div class="row">
      <div class="col-sm-4">
      </div>
      <% 4.times do %>
        <div class="col-sm-1">
          <h3><a href="#" class="btn btn-default"><%= @feelings.sample.word %></a></h3>
        </div>
      <% end %>
    </div>
  <% end %>

  <div class="row">
    <div class="col-sm-4">
    </div>
  </div>

  <div class="row">
    <div class="col-sm-5">
    </div>
    <div class="col-sm-3">
      <%= form_for :location, url: "#", html: {id: "search"} do |f| %>
        <p><%= f.text_field :location, :placeholder => "zip" %><%= f.submit "Search" %></p>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div id="demo">
    <div>
  </div>
</div>

<div class="container-fluid" id="results-container">
  <div class="row">
    <div class="col-sm-4">
    </div>
    <div class="col-sm-4">
      <h3>These people may be able to help...</h3>
    </div>
  </div>

  <% Provider.all.each do |provider| %>
    <%= render partial: "providers/result", layout: false, locals: {provider: provider} %>
  <% end %>

</div>
